<div class='all-container'>
  <div class='single-container'>
    <%= form_for login_path do %>
      <%= label_tag :email %><br>
      <%= text_field_tag :email %><br>
      <%= label_tag :password %><br>
      <%= password_field_tag :password %><br>
      <%= submit_tag "Log In" %>
    <% end %>
  </div>
</div>